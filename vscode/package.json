{
  "name": "mux",
  "displayName": "mux",
  "description": "Open mux workspaces directly from VS Code",
  "version": "0.1.1",
  "publisher": "coder",
  "icon": "icon.png",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "mux",
    "workspace",
    "remote",
    "ssh",
    "development"
  ],
  "activationEvents": [
    "onCommand:mux.openWorkspace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mux.openWorkspace",
        "title": "mux: Open Workspace"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "bun run compile",
    "compile": "node esbuild.config.js",
    "watch": "node esbuild.config.js --watch",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^2.22.0",
    "esbuild": "^0.27.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coder/mux.git",
    "directory": "vscode"
  },
  "license": "AGPL-3.0-only",
  "packageManager": "bun@1.1.42"
}
